#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Volumes/Code/quick-func/node_modules/.pnpm/mini-svg-data-uri@1.4.3/node_modules/mini-svg-data-uri/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/mini-svg-data-uri@1.4.3/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/node_modules:/Volumes/Code/quick-func/node_modules:/Volumes/Code/node_modules:/Volumes/node_modules:/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/gatsby-plugin-sharp@4.8.1_gatsby@4.8.1/node_modules/gatsby-plugin-sharp/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/gatsby-plugin-sharp@4.8.1_gatsby@4.8.1/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Volumes/Code/quick-func/node_modules/.pnpm/mini-svg-data-uri@1.4.3/node_modules/mini-svg-data-uri/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/mini-svg-data-uri@1.4.3/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/node_modules:/Volumes/Code/quick-func/node_modules:/Volumes/Code/node_modules:/Volumes/node_modules:/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/gatsby-plugin-sharp@4.8.1_gatsby@4.8.1/node_modules/gatsby-plugin-sharp/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/gatsby-plugin-sharp@4.8.1_gatsby@4.8.1/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../mini-svg-data-uri@1.4.3/node_modules/mini-svg-data-uri/cli.js" "$@"
else
  exec node  "$basedir/../../../../../mini-svg-data-uri@1.4.3/node_modules/mini-svg-data-uri/cli.js" "$@"
fi
