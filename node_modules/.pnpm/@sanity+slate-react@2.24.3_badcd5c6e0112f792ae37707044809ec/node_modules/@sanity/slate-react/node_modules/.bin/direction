#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Volumes/Code/quick-func/node_modules/.pnpm/direction@1.0.4/node_modules/direction/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/direction@1.0.4/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/node_modules:/Volumes/Code/quick-func/node_modules:/Volumes/Code/node_modules:/Volumes/node_modules:/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/@sanity+slate-react@2.24.3_badcd5c6e0112f792ae37707044809ec/node_modules/@sanity/slate-react/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/@sanity+slate-react@2.24.3_badcd5c6e0112f792ae37707044809ec/node_modules/@sanity/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/@sanity+slate-react@2.24.3_badcd5c6e0112f792ae37707044809ec/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Volumes/Code/quick-func/node_modules/.pnpm/direction@1.0.4/node_modules/direction/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/direction@1.0.4/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/node_modules:/Volumes/Code/quick-func/node_modules:/Volumes/Code/node_modules:/Volumes/node_modules:/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/@sanity+slate-react@2.24.3_badcd5c6e0112f792ae37707044809ec/node_modules/@sanity/slate-react/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/@sanity+slate-react@2.24.3_badcd5c6e0112f792ae37707044809ec/node_modules/@sanity/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/@sanity+slate-react@2.24.3_badcd5c6e0112f792ae37707044809ec/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../direction@1.0.4/node_modules/direction/cli.js" "$@"
else
  exec node  "$basedir/../../../../../../direction@1.0.4/node_modules/direction/cli.js" "$@"
fi
