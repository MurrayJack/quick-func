#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Volumes/Code/quick-func/node_modules/.pnpm/gunzip-maybe@1.4.2/node_modules/gunzip-maybe/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/gunzip-maybe@1.4.2/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/node_modules:/Volumes/Code/quick-func/node_modules:/Volumes/Code/node_modules:/Volumes/node_modules:/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/@sanity+import@2.27.0/node_modules/@sanity/import/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/@sanity+import@2.27.0/node_modules/@sanity/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/@sanity+import@2.27.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Volumes/Code/quick-func/node_modules/.pnpm/gunzip-maybe@1.4.2/node_modules/gunzip-maybe/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/gunzip-maybe@1.4.2/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/node_modules:/Volumes/Code/quick-func/node_modules:/Volumes/Code/node_modules:/Volumes/node_modules:/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/@sanity+import@2.27.0/node_modules/@sanity/import/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/@sanity+import@2.27.0/node_modules/@sanity/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/@sanity+import@2.27.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../gunzip-maybe@1.4.2/node_modules/gunzip-maybe/bin.js" "$@"
else
  exec node  "$basedir/../../../../../../gunzip-maybe@1.4.2/node_modules/gunzip-maybe/bin.js" "$@"
fi
