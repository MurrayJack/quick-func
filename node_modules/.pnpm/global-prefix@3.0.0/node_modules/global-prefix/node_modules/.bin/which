#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Volumes/Code/quick-func/node_modules/.pnpm/which@1.3.1/node_modules/which/bin/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/which@1.3.1/node_modules/which/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/which@1.3.1/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/node_modules:/Volumes/Code/quick-func/node_modules:/Volumes/Code/node_modules:/Volumes/node_modules:/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/global-prefix@3.0.0/node_modules/global-prefix/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/global-prefix@3.0.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Volumes/Code/quick-func/node_modules/.pnpm/which@1.3.1/node_modules/which/bin/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/which@1.3.1/node_modules/which/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/which@1.3.1/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/node_modules:/Volumes/Code/quick-func/node_modules:/Volumes/Code/node_modules:/Volumes/node_modules:/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/global-prefix@3.0.0/node_modules/global-prefix/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/global-prefix@3.0.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../which@1.3.1/node_modules/which/bin/which" "$@"
else
  exec node  "$basedir/../../../../../which@1.3.1/node_modules/which/bin/which" "$@"
fi
