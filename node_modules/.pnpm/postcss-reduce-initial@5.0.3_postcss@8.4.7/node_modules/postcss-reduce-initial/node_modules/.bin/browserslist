#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Volumes/Code/quick-func/node_modules/.pnpm/browserslist@4.19.3/node_modules/browserslist/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/browserslist@4.19.3/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/node_modules:/Volumes/Code/quick-func/node_modules:/Volumes/Code/node_modules:/Volumes/node_modules:/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/postcss-reduce-initial@5.0.3_postcss@8.4.7/node_modules/postcss-reduce-initial/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/postcss-reduce-initial@5.0.3_postcss@8.4.7/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Volumes/Code/quick-func/node_modules/.pnpm/browserslist@4.19.3/node_modules/browserslist/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/browserslist@4.19.3/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/node_modules:/Volumes/Code/quick-func/node_modules:/Volumes/Code/node_modules:/Volumes/node_modules:/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/postcss-reduce-initial@5.0.3_postcss@8.4.7/node_modules/postcss-reduce-initial/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/postcss-reduce-initial@5.0.3_postcss@8.4.7/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../browserslist@4.19.3/node_modules/browserslist/cli.js" "$@"
else
  exec node  "$basedir/../../../../../browserslist@4.19.3/node_modules/browserslist/cli.js" "$@"
fi
