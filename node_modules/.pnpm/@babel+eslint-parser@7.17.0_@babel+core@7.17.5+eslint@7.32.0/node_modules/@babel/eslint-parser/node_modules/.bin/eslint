#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Volumes/Code/quick-func/node_modules/.pnpm/eslint@7.32.0/node_modules/eslint/bin/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/eslint@7.32.0/node_modules/eslint/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/eslint@7.32.0/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/node_modules:/Volumes/Code/quick-func/node_modules:/Volumes/Code/node_modules:/Volumes/node_modules:/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/@babel+eslint-parser@7.17.0_@babel+core@7.17.5+eslint@7.32.0/node_modules/@babel/eslint-parser/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/@babel+eslint-parser@7.17.0_@babel+core@7.17.5+eslint@7.32.0/node_modules/@babel/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/@babel+eslint-parser@7.17.0_@babel+core@7.17.5+eslint@7.32.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Volumes/Code/quick-func/node_modules/.pnpm/eslint@7.32.0/node_modules/eslint/bin/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/eslint@7.32.0/node_modules/eslint/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/eslint@7.32.0/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/node_modules:/Volumes/Code/quick-func/node_modules:/Volumes/Code/node_modules:/Volumes/node_modules:/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/@babel+eslint-parser@7.17.0_@babel+core@7.17.5+eslint@7.32.0/node_modules/@babel/eslint-parser/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/@babel+eslint-parser@7.17.0_@babel+core@7.17.5+eslint@7.32.0/node_modules/@babel/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/@babel+eslint-parser@7.17.0_@babel+core@7.17.5+eslint@7.32.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../eslint@7.32.0/node_modules/eslint/bin/eslint.js" "$@"
else
  exec node  "$basedir/../../../../../../eslint@7.32.0/node_modules/eslint/bin/eslint.js" "$@"
fi
