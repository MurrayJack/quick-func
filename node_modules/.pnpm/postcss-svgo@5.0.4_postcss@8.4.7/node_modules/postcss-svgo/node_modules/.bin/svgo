#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Volumes/Code/quick-func/node_modules/.pnpm/svgo@2.8.0/node_modules/svgo/bin/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/svgo@2.8.0/node_modules/svgo/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/svgo@2.8.0/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/node_modules:/Volumes/Code/quick-func/node_modules:/Volumes/Code/node_modules:/Volumes/node_modules:/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/postcss-svgo@5.0.4_postcss@8.4.7/node_modules/postcss-svgo/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/postcss-svgo@5.0.4_postcss@8.4.7/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Volumes/Code/quick-func/node_modules/.pnpm/svgo@2.8.0/node_modules/svgo/bin/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/svgo@2.8.0/node_modules/svgo/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/svgo@2.8.0/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/node_modules:/Volumes/Code/quick-func/node_modules:/Volumes/Code/node_modules:/Volumes/node_modules:/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/postcss-svgo@5.0.4_postcss@8.4.7/node_modules/postcss-svgo/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/postcss-svgo@5.0.4_postcss@8.4.7/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../svgo@2.8.0/node_modules/svgo/bin/svgo" "$@"
else
  exec node  "$basedir/../../../../../svgo@2.8.0/node_modules/svgo/bin/svgo" "$@"
fi
