#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Volumes/Code/quick-func/node_modules/.pnpm/resolve@1.22.0/node_modules/resolve/bin/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/resolve@1.22.0/node_modules/resolve/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/resolve@1.22.0/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/node_modules:/Volumes/Code/quick-func/node_modules:/Volumes/Code/node_modules:/Volumes/node_modules:/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/@sanity+webpack-integration@2.27.0_webpack@3.12.0/node_modules/@sanity/webpack-integration/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/@sanity+webpack-integration@2.27.0_webpack@3.12.0/node_modules/@sanity/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/@sanity+webpack-integration@2.27.0_webpack@3.12.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Volumes/Code/quick-func/node_modules/.pnpm/resolve@1.22.0/node_modules/resolve/bin/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/resolve@1.22.0/node_modules/resolve/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/resolve@1.22.0/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/node_modules:/Volumes/Code/quick-func/node_modules:/Volumes/Code/node_modules:/Volumes/node_modules:/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/@sanity+webpack-integration@2.27.0_webpack@3.12.0/node_modules/@sanity/webpack-integration/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/@sanity+webpack-integration@2.27.0_webpack@3.12.0/node_modules/@sanity/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/@sanity+webpack-integration@2.27.0_webpack@3.12.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../resolve@1.22.0/node_modules/resolve/bin/resolve" "$@"
else
  exec node  "$basedir/../../../../../../resolve@1.22.0/node_modules/resolve/bin/resolve" "$@"
fi
