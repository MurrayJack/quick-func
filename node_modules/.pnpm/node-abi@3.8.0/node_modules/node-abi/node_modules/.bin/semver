#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Volumes/Code/quick-func/node_modules/.pnpm/semver@7.3.5/node_modules/semver/bin/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/semver@7.3.5/node_modules/semver/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/semver@7.3.5/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/node_modules:/Volumes/Code/quick-func/node_modules:/Volumes/Code/node_modules:/Volumes/node_modules:/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/node-abi@3.8.0/node_modules/node-abi/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/node-abi@3.8.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Volumes/Code/quick-func/node_modules/.pnpm/semver@7.3.5/node_modules/semver/bin/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/semver@7.3.5/node_modules/semver/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/semver@7.3.5/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/node_modules:/Volumes/Code/quick-func/node_modules:/Volumes/Code/node_modules:/Volumes/node_modules:/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/node-abi@3.8.0/node_modules/node-abi/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/node-abi@3.8.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../semver@7.3.5/node_modules/semver/bin/semver.js" "$@"
else
  exec node  "$basedir/../../../../../semver@7.3.5/node_modules/semver/bin/semver.js" "$@"
fi
