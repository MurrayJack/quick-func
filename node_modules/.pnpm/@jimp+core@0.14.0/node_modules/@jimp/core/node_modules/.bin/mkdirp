#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Volumes/Code/quick-func/node_modules/.pnpm/mkdirp@0.5.5/node_modules/mkdirp/bin/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/mkdirp@0.5.5/node_modules/mkdirp/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/mkdirp@0.5.5/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/node_modules:/Volumes/Code/quick-func/node_modules:/Volumes/Code/node_modules:/Volumes/node_modules:/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/@jimp+core@0.14.0/node_modules/@jimp/core/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/@jimp+core@0.14.0/node_modules/@jimp/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/@jimp+core@0.14.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Volumes/Code/quick-func/node_modules/.pnpm/mkdirp@0.5.5/node_modules/mkdirp/bin/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/mkdirp@0.5.5/node_modules/mkdirp/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/mkdirp@0.5.5/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/node_modules:/Volumes/Code/quick-func/node_modules:/Volumes/Code/node_modules:/Volumes/node_modules:/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/@jimp+core@0.14.0/node_modules/@jimp/core/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/@jimp+core@0.14.0/node_modules/@jimp/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/@jimp+core@0.14.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../mkdirp@0.5.5/node_modules/mkdirp/bin/cmd.js" "$@"
else
  exec node  "$basedir/../../../../../../mkdirp@0.5.5/node_modules/mkdirp/bin/cmd.js" "$@"
fi
