#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Volumes/Code/quick-func/node_modules/.pnpm/uglify-js@2.8.29/node_modules/uglify-js/bin/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/uglify-js@2.8.29/node_modules/uglify-js/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/uglify-js@2.8.29/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/node_modules:/Volumes/Code/quick-func/node_modules:/Volumes/Code/node_modules:/Volumes/node_modules:/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/uglifyjs-webpack-plugin@0.4.6_webpack@3.12.0/node_modules/uglifyjs-webpack-plugin/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/uglifyjs-webpack-plugin@0.4.6_webpack@3.12.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Volumes/Code/quick-func/node_modules/.pnpm/uglify-js@2.8.29/node_modules/uglify-js/bin/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/uglify-js@2.8.29/node_modules/uglify-js/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/uglify-js@2.8.29/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/node_modules:/Volumes/Code/quick-func/node_modules:/Volumes/Code/node_modules:/Volumes/node_modules:/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/uglifyjs-webpack-plugin@0.4.6_webpack@3.12.0/node_modules/uglifyjs-webpack-plugin/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/uglifyjs-webpack-plugin@0.4.6_webpack@3.12.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../uglify-js@2.8.29/node_modules/uglify-js/bin/uglifyjs" "$@"
else
  exec node  "$basedir/../../../../../uglify-js@2.8.29/node_modules/uglify-js/bin/uglifyjs" "$@"
fi
