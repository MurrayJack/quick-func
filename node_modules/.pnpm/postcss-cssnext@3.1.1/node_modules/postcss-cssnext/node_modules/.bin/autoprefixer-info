#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Volumes/Code/quick-func/node_modules/.pnpm/autoprefixer@7.2.6/node_modules/autoprefixer/bin/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/autoprefixer@7.2.6/node_modules/autoprefixer/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/autoprefixer@7.2.6/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/node_modules:/Volumes/Code/quick-func/node_modules:/Volumes/Code/node_modules:/Volumes/node_modules:/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/postcss-cssnext@3.1.1/node_modules/postcss-cssnext/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/postcss-cssnext@3.1.1/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Volumes/Code/quick-func/node_modules/.pnpm/autoprefixer@7.2.6/node_modules/autoprefixer/bin/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/autoprefixer@7.2.6/node_modules/autoprefixer/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/autoprefixer@7.2.6/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/node_modules:/Volumes/Code/quick-func/node_modules:/Volumes/Code/node_modules:/Volumes/node_modules:/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/postcss-cssnext@3.1.1/node_modules/postcss-cssnext/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/postcss-cssnext@3.1.1/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../autoprefixer@7.2.6/node_modules/autoprefixer/bin/autoprefixer-info" "$@"
else
  exec node  "$basedir/../../../../../autoprefixer@7.2.6/node_modules/autoprefixer/bin/autoprefixer-info" "$@"
fi
