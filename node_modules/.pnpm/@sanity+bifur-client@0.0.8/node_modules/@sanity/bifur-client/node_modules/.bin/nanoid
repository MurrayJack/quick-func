#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Volumes/Code/quick-func/node_modules/.pnpm/nanoid@3.3.1/node_modules/nanoid/bin/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/nanoid@3.3.1/node_modules/nanoid/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/nanoid@3.3.1/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/node_modules:/Volumes/Code/quick-func/node_modules:/Volumes/Code/node_modules:/Volumes/node_modules:/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/@sanity+bifur-client@0.0.8/node_modules/@sanity/bifur-client/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/@sanity+bifur-client@0.0.8/node_modules/@sanity/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/@sanity+bifur-client@0.0.8/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Volumes/Code/quick-func/node_modules/.pnpm/nanoid@3.3.1/node_modules/nanoid/bin/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/nanoid@3.3.1/node_modules/nanoid/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/nanoid@3.3.1/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/node_modules:/Volumes/Code/quick-func/node_modules:/Volumes/Code/node_modules:/Volumes/node_modules:/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/@sanity+bifur-client@0.0.8/node_modules/@sanity/bifur-client/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/@sanity+bifur-client@0.0.8/node_modules/@sanity/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/@sanity+bifur-client@0.0.8/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../nanoid@3.3.1/node_modules/nanoid/bin/nanoid.cjs" "$@"
else
  exec node  "$basedir/../../../../../../nanoid@3.3.1/node_modules/nanoid/bin/nanoid.cjs" "$@"
fi
