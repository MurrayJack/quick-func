#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Volumes/Code/quick-func/node_modules/.pnpm/esprima@4.0.1/node_modules/esprima/bin/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/esprima@4.0.1/node_modules/esprima/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/esprima@4.0.1/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/node_modules:/Volumes/Code/quick-func/node_modules:/Volumes/Code/node_modules:/Volumes/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Volumes/Code/quick-func/node_modules/.pnpm/esprima@4.0.1/node_modules/esprima/bin/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/esprima@4.0.1/node_modules/esprima/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/esprima@4.0.1/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/node_modules:/Volumes/Code/quick-func/node_modules:/Volumes/Code/node_modules:/Volumes/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/esvalidate.js" "$@"
else
  exec node  "$basedir/../../bin/esvalidate.js" "$@"
fi
