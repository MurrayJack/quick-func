#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Volumes/Code/quick-func/node_modules/.pnpm/mime@2.6.0/node_modules/mime/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/mime@2.6.0/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/node_modules:/Volumes/Code/quick-func/node_modules:/Volumes/Code/node_modules:/Volumes/node_modules:/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/st@2.0.0/node_modules/st/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/st@2.0.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Volumes/Code/quick-func/node_modules/.pnpm/mime@2.6.0/node_modules/mime/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/mime@2.6.0/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/node_modules:/Volumes/Code/quick-func/node_modules:/Volumes/Code/node_modules:/Volumes/node_modules:/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/st@2.0.0/node_modules/st/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/st@2.0.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../mime@2.6.0/node_modules/mime/cli.js" "$@"
else
  exec node  "$basedir/../../../../../mime@2.6.0/node_modules/mime/cli.js" "$@"
fi
