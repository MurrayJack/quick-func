#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Volumes/Code/quick-func/node_modules/.pnpm/webpack@5.69.1/node_modules/webpack/bin/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/webpack@5.69.1/node_modules/webpack/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/webpack@5.69.1/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/node_modules:/Volumes/Code/quick-func/node_modules:/Volumes/Code/node_modules:/Volumes/node_modules:/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/css-minimizer-webpack-plugin@2.0.0_webpack@5.69.1/node_modules/css-minimizer-webpack-plugin/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/css-minimizer-webpack-plugin@2.0.0_webpack@5.69.1/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Volumes/Code/quick-func/node_modules/.pnpm/webpack@5.69.1/node_modules/webpack/bin/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/webpack@5.69.1/node_modules/webpack/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/webpack@5.69.1/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/node_modules:/Volumes/Code/quick-func/node_modules:/Volumes/Code/node_modules:/Volumes/node_modules:/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/css-minimizer-webpack-plugin@2.0.0_webpack@5.69.1/node_modules/css-minimizer-webpack-plugin/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/css-minimizer-webpack-plugin@2.0.0_webpack@5.69.1/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../webpack@5.69.1/node_modules/webpack/bin/webpack.js" "$@"
else
  exec node  "$basedir/../../../../../webpack@5.69.1/node_modules/webpack/bin/webpack.js" "$@"
fi
