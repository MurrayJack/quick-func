#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Volumes/Code/quick-func/node_modules/.pnpm/gatsby@4.8.1_react-dom@17.0.2+react@17.0.2/node_modules/gatsby/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/gatsby@4.8.1_react-dom@17.0.2+react@17.0.2/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/node_modules:/Volumes/Code/quick-func/node_modules:/Volumes/Code/node_modules:/Volumes/node_modules:/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/babel-plugin-remove-graphql-queries@4.8.1_gatsby@4.8.1/node_modules/babel-plugin-remove-graphql-queries/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/babel-plugin-remove-graphql-queries@4.8.1_gatsby@4.8.1/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Volumes/Code/quick-func/node_modules/.pnpm/gatsby@4.8.1_react-dom@17.0.2+react@17.0.2/node_modules/gatsby/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/gatsby@4.8.1_react-dom@17.0.2+react@17.0.2/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/node_modules:/Volumes/Code/quick-func/node_modules:/Volumes/Code/node_modules:/Volumes/node_modules:/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/babel-plugin-remove-graphql-queries@4.8.1_gatsby@4.8.1/node_modules/babel-plugin-remove-graphql-queries/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/babel-plugin-remove-graphql-queries@4.8.1_gatsby@4.8.1/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../gatsby@4.8.1_react-dom@17.0.2+react@17.0.2/node_modules/gatsby/cli.js" "$@"
else
  exec node  "$basedir/../../../../../gatsby@4.8.1_react-dom@17.0.2+react@17.0.2/node_modules/gatsby/cli.js" "$@"
fi
