#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Volumes/Code/quick-func/node_modules/.pnpm/ansi-html-community@0.0.8/node_modules/ansi-html-community/bin/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/ansi-html-community@0.0.8/node_modules/ansi-html-community/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/ansi-html-community@0.0.8/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/node_modules:/Volumes/Code/quick-func/node_modules:/Volumes/Code/node_modules:/Volumes/node_modules:/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/webpack-hot-middleware@2.25.1/node_modules/webpack-hot-middleware/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/webpack-hot-middleware@2.25.1/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Volumes/Code/quick-func/node_modules/.pnpm/ansi-html-community@0.0.8/node_modules/ansi-html-community/bin/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/ansi-html-community@0.0.8/node_modules/ansi-html-community/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/ansi-html-community@0.0.8/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/node_modules:/Volumes/Code/quick-func/node_modules:/Volumes/Code/node_modules:/Volumes/node_modules:/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/webpack-hot-middleware@2.25.1/node_modules/webpack-hot-middleware/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/webpack-hot-middleware@2.25.1/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../ansi-html-community@0.0.8/node_modules/ansi-html-community/bin/ansi-html" "$@"
else
  exec node  "$basedir/../../../../../ansi-html-community@0.0.8/node_modules/ansi-html-community/bin/ansi-html" "$@"
fi
