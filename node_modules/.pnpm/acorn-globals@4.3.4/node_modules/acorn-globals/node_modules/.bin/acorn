#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Volumes/Code/quick-func/node_modules/.pnpm/acorn@6.4.2/node_modules/acorn/bin/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/acorn@6.4.2/node_modules/acorn/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/acorn@6.4.2/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/node_modules:/Volumes/Code/quick-func/node_modules:/Volumes/Code/node_modules:/Volumes/node_modules:/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/acorn-globals@4.3.4/node_modules/acorn-globals/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/acorn-globals@4.3.4/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Volumes/Code/quick-func/node_modules/.pnpm/acorn@6.4.2/node_modules/acorn/bin/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/acorn@6.4.2/node_modules/acorn/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/acorn@6.4.2/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/node_modules:/Volumes/Code/quick-func/node_modules:/Volumes/Code/node_modules:/Volumes/node_modules:/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/acorn-globals@4.3.4/node_modules/acorn-globals/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/acorn-globals@4.3.4/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../acorn@6.4.2/node_modules/acorn/bin/acorn" "$@"
else
  exec node  "$basedir/../../../../../acorn@6.4.2/node_modules/acorn/bin/acorn" "$@"
fi
