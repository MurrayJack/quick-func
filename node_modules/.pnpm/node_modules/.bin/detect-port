#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Volumes/Code/quick-func/node_modules/.pnpm/detect-port@1.3.0/node_modules/detect-port/bin/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/detect-port@1.3.0/node_modules/detect-port/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/detect-port@1.3.0/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/node_modules:/Volumes/Code/quick-func/node_modules:/Volumes/Code/node_modules:/Volumes/node_modules:/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/node_modules/detect-port/bin/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/node_modules/detect-port/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Volumes/Code/quick-func/node_modules/.pnpm/detect-port@1.3.0/node_modules/detect-port/bin/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/detect-port@1.3.0/node_modules/detect-port/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/detect-port@1.3.0/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/node_modules:/Volumes/Code/quick-func/node_modules:/Volumes/Code/node_modules:/Volumes/node_modules:/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/node_modules/detect-port/bin/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/node_modules/detect-port/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../detect-port/bin/detect-port" "$@"
else
  exec node  "$basedir/../detect-port/bin/detect-port" "$@"
fi
