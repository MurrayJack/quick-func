#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Volumes/Code/quick-func/node_modules/.pnpm/@sanity+server@2.27.0_react-dom@17.0.2+react@17.0.2/node_modules/@sanity/server/bin/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/@sanity+server@2.27.0_react-dom@17.0.2+react@17.0.2/node_modules/@sanity/server/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/@sanity+server@2.27.0_react-dom@17.0.2+react@17.0.2/node_modules/@sanity/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/@sanity+server@2.27.0_react-dom@17.0.2+react@17.0.2/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/node_modules:/Volumes/Code/quick-func/node_modules:/Volumes/Code/node_modules:/Volumes/node_modules:/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/node_modules/@sanity/server/bin/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/node_modules/@sanity/server/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/node_modules/@sanity/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Volumes/Code/quick-func/node_modules/.pnpm/@sanity+server@2.27.0_react-dom@17.0.2+react@17.0.2/node_modules/@sanity/server/bin/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/@sanity+server@2.27.0_react-dom@17.0.2+react@17.0.2/node_modules/@sanity/server/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/@sanity+server@2.27.0_react-dom@17.0.2+react@17.0.2/node_modules/@sanity/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/@sanity+server@2.27.0_react-dom@17.0.2+react@17.0.2/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/node_modules:/Volumes/Code/quick-func/node_modules:/Volumes/Code/node_modules:/Volumes/node_modules:/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/node_modules/@sanity/server/bin/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/node_modules/@sanity/server/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/node_modules/@sanity/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@sanity/server/bin/sanity-server.js" "$@"
else
  exec node  "$basedir/../@sanity/server/bin/sanity-server.js" "$@"
fi
