#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Volumes/Code/quick-func/node_modules/.pnpm/eslint@8.10.0/node_modules/eslint/bin/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/eslint@8.10.0/node_modules/eslint/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/eslint@8.10.0/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/node_modules:/Volumes/Code/quick-func/node_modules:/Volumes/Code/node_modules:/Volumes/node_modules:/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/@typescript-eslint+eslint-plugin@5.12.1_6a3ee9a665bfcbc5c9de1db6da4c94f0/node_modules/@typescript-eslint/eslint-plugin/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/@typescript-eslint+eslint-plugin@5.12.1_6a3ee9a665bfcbc5c9de1db6da4c94f0/node_modules/@typescript-eslint/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/@typescript-eslint+eslint-plugin@5.12.1_6a3ee9a665bfcbc5c9de1db6da4c94f0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Volumes/Code/quick-func/node_modules/.pnpm/eslint@8.10.0/node_modules/eslint/bin/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/eslint@8.10.0/node_modules/eslint/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/eslint@8.10.0/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/node_modules:/Volumes/Code/quick-func/node_modules:/Volumes/Code/node_modules:/Volumes/node_modules:/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/@typescript-eslint+eslint-plugin@5.12.1_6a3ee9a665bfcbc5c9de1db6da4c94f0/node_modules/@typescript-eslint/eslint-plugin/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/@typescript-eslint+eslint-plugin@5.12.1_6a3ee9a665bfcbc5c9de1db6da4c94f0/node_modules/@typescript-eslint/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/@typescript-eslint+eslint-plugin@5.12.1_6a3ee9a665bfcbc5c9de1db6da4c94f0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../eslint@8.10.0/node_modules/eslint/bin/eslint.js" "$@"
else
  exec node  "$basedir/../../../../../../eslint@8.10.0/node_modules/eslint/bin/eslint.js" "$@"
fi
