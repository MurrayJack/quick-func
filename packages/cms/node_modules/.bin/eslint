#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Volumes/Code/quick-func/node_modules/.pnpm/eslint@8.10.0/node_modules/eslint/bin/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/eslint@8.10.0/node_modules/eslint/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/eslint@8.10.0/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/node_modules:/Volumes/Code/quick-func/node_modules:/Volumes/Code/node_modules:/Volumes/node_modules:/node_modules:/Volumes/Code/quick-func/packages/cms/node_modules:/Volumes/Code/quick-func/packages/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Volumes/Code/quick-func/node_modules/.pnpm/eslint@8.10.0/node_modules/eslint/bin/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/eslint@8.10.0/node_modules/eslint/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/eslint@8.10.0/node_modules:/Volumes/Code/quick-func/node_modules/.pnpm/node_modules:/Volumes/Code/quick-func/node_modules:/Volumes/Code/node_modules:/Volumes/node_modules:/node_modules:/Volumes/Code/quick-func/packages/cms/node_modules:/Volumes/Code/quick-func/packages/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/eslint@8.10.0/node_modules/eslint/bin/eslint.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/eslint@8.10.0/node_modules/eslint/bin/eslint.js" "$@"
fi
